page ,.container{
   width: 750rpx;
    height: 100%;
    overflow-x: hidden;
    overflow-y: auto;
    background: #f4f4f4;
    position: relative;
}
.help-head{
  background-color: #fff;
  padding: 30rpx;
  font-size: 35rpx;
  color: #009944;
}

 .list-cell-bd{
    width: 100%;
    background-size: 16rpx auto,750rpx auto;
    margin: 1rpx 0; 
    border-bottom: 1px solid #eee;
    height:90rpx;
    line-height: 70rpx;
    padding-left: 40rpx;
    background-color:white;
    display:flex;
    flex-direction:row;
}

.list-cell-bd .arrow{
  width: 10px;
  height: 10px;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  position: absolute;
  right: 20px;
  transform: rotate(45deg);
  margin-top:30rpx;
}
